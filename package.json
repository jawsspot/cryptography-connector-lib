{
  "name": "crypto-drivers",
  "version": "0.0.1",
  "description": "Работа с плагинами Rutoken, Jacarta, CryptoPro реализована через классы-драйверы.",
  "main": "dist/index.js",
  "scripts": {
    "clear": "rimraf dist/",
    "compile": "tsc -p tsconfig.json",
    "build:babel": "babel src -d dist",
    "build:types": "tsc -d --declarationDir dist  --emitDeclarationOnly",
    "build": "npm run clear && npm run build:babel && npm run build:types"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jawsspot/cryptography-connector-lib.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.5",
    "@babel/preset-env": "7.15.6",
    "rimraf": "3.0.2",
    "typescript": "4.8.4"
  },
  "typings": "./dist/index.d.ts"
}